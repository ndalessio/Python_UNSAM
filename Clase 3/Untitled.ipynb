    "def tiene_a(expresion):\n",
    "    n = len(expresion)\n",
    "    i = 0\n",
    "    while i<n:\n",
    "        if expresion[i] == 'a':\n",
    "            return True\n",
    "        else:\n",
    "            return False\n",
    "        i += 1\n",
    "\n",
    "#tiene_a('UNSAM 2020')\n",
    "#tiene_a('abracadabra')\n",
    "tiene_a('La novela 1984 de George Orwell')"
